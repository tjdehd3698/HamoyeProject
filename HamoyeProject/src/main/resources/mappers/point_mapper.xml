<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sql.hamoye.point_mapper">
	<insert id="insertPoint" parameterType="point">
		<selectKey keyProperty="pointId" resultType="string" order="BEFORE">
			select 'pnt'||point_seq.nextval from dual
		</selectKey>
		
		insert into 
		point(point_id,total_point)
		values(#{pointId},#{totalPoint})
	</insert>
	
	
	
</mapper>









